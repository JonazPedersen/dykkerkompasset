@using DKrepo
@model List<Spot>
@{
    ViewBag.Title = "VisPosition";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@foreach (Spot s in Model)
{    
    <div class="row">
        <table>
            <tr>
                <td class="large-6 medium-6 small-6 .columns">@s.Navn</td>
                <td class="large-3 medium-3 small-3 .columns">@s.Long</td>
                <td class="large-2 medium-2 small-2 .columns">@s.Lati</td>
                <td class="large-1 medium-1 small-1 .columns"><a href="/Home/ShowSpot/@s.ID/"><img src="~/Content/ikoner/videre.png" width="30" /></a></td>
            </tr>
        </table>
    </div>
}